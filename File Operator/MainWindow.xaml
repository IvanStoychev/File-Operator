<Window x:Class="File_Operator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:File_Operator"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ToolBarPanel HorizontalAlignment="Stretch" Height="30" VerticalAlignment="Top">
            <ToolBarTray HorizontalAlignment="Stretch" Height="30" VerticalAlignment="Stretch">
                <ToolBar HorizontalAlignment="Stretch" Height="30" VerticalAlignment="Stretch">
                    
                </ToolBar>
            </ToolBarTray>
        </ToolBarPanel>
        <Button Content="Operate" HorizontalAlignment="Center" Margin="0,385,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
    </Grid>
</Window>
